{
    "cSpell.words": ["xabcdef"],
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "editor.formatOnType": true,
    "files.watcherExclude": {
        "**/target": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "bin/": true,
        "build/": true
    },
    "java.autobuild.enabled": true,
    "java.cleanup.actions": [
        "addDeprecated",
        "addFinalModifier",
        "addOverride",
        "instanceofPatternMatch",
        "invertEquals",
        "lambdaExpression",
        "lambdaExpressionFromAnonymousClass",
        "stringConcatToTextBlock",
        "switchExpression",
        "tryWithResource"
    ],
    "java.codeAction.sortMembers.avoidVolatileChanges": false,
    "java.codeGeneration.hashCodeEquals.useInstanceof": true,
    "java.codeGeneration.hashCodeEquals.useJava7Objects": true,
    "java.codeGeneration.toString.codeStyle": "STRING_CONCATENATION",
    "java.compile.nullAnalysis.mode": "automatic",
    "java.completion.enabled": true,
    "java.completion.matchCase": "firstLetter",
    "java.configuration.runtimes": [],
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.dependency.packagePresentation": "hierarchical",
    "java.format.enabled": true,
    "java.format.onType.enabled": true,
    "java.format.settings.profile": "GoogleStyle",
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.inlayHints.parameterNames.enabled": "none",
    "java.saveActions.organizeImports": true,
    "java.jdt.ls.lombokSupport.enabled": false,
    "java.jdt.ls.protobufSupport.enabled": false,
    "java.debug.logLevel": "info",
    "java.sharedIndexes.enabled": "off",
    "java.jdt.ls.androidSupport.enabled": "off",
    "java.project.encoding": "warning",
    "java.import.maven.enabled": false,
    "java.completion.chain.enabled": true,
    "java.quickfix.showAt": "problem",
    "java.referencesCodeLens.enabled": true,
    "java.implementationsCodeLens.enabled": true,
    "java.typeHierarchy.lazyLoad": true,
    "spring-boot.ls.problem.boot2.JAVA_CONSTRUCTOR_PARAMETER_INJECTION": "HINT"
}
